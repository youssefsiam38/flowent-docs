{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Flowent API Gateway",
  "description": "Documentation for the Flowent API Gateway",
  "colors": {
    "primary": "#0066CC",
    "light": "#0088FF",
    "dark": "#0052A3"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Concepts",
            "pages": [
              "api-concepts"
            ]
          },
          {
            "group": "Integration",
            "pages": [
              "api_gateway/MIGRATION"
            ]
          },
          {
            "group": "Examples",
            "pages": [
              "api_gateway/examples/README",
              "api_gateway/examples/python/README"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Specification",
            "pages": [
              "api-reference/introduction"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/janan-tech/flowent",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/janan-tech/flowent"
      }
    ]
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/janan-tech/flowent"
    }
  }
}
